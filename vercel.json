{
  "buildCommand": "cd src/frontend && npm install && npm run build",
  "outputDirectory": "src/frontend/dist",
  "devCommand": "cd src/frontend && npm run dev",
  "rewrites": [
    { "source": "/api/(.*)", "destination": "/api/serverless.js" },
    { "source": "/(.*)", "destination": "/index.html" }
  ]
}